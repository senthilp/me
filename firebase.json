{
  "hosting": {
    "public": "public",
    "ignore": [
      ".gitignore",
      "*.pem"
    ],
    "trailingSlash": false,
    "rewrites": [{
      "source": "/offexp",
      "destination": "/offline/index.html"
    }, {
      "source": "/offline/resources",
      "destination": "/offline/resources.json"
    }],
    "headers": [{
      "source" : "/",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "no-cache"
      }]
    },{
      "source" : "/sw.js",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "no-cache"
      }]      
    },{
      "source" : "/images/*.*",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "public, max-age=86400"
      }]      
    }]
  }
}
